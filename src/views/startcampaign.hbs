<div class="main-container">
        
<div class="tab-content">
                      
                      <div class="tab-pane active" id="signin">
                        <div class="row">

                          <div class="col-lg-5">
                            <h4>Start a Campaign</h4>
                            <form method="POST" action="/startcampaign" role="form" >
                              <div class="form-group">
                              <label for="title">Title<span>*</span></label>
                              <input name="title" type="text" class="form-control" id="title">
                              </div>
                            <div class="form-group>
                              <label for="goal">Goal<span>*</span></label>
                              <input name="goal" type="text" class="form-control" id="goal">
                            </div>
                            <div class="form-group">
                              <label for="lname">Catagory<span>*</span></label>
                              <input name="catagory" type="text" class="form-control" id="catagory">
                            </div>
                            <div class="form-group">
                              <label for="company">Location</label>
                              <input name="location" type="text" class="form-control" id="location">
                            </div>
                            <form role="form" >
                              <div class="form-group">
                              <label for="story">Story</label>
                                <div class="checkbox checkbox-right">
                                <!-- <label><input type="checkbox">Ship to billing address?</label> -->
                              </div>
                              <textarea name="story" class="form-control" id="story" rows="3" cols="10"></textarea>
                            </div>
 
                            <div class="form-group">
                              <button type="submit" class="btn btn-default btn-lg">Submit</button>
                            </div>


                            </form>
                          </div>
                        </div>
                        </div>
              
  

                       </div>

